     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "a = np.array([1,2,3])\n",
    "b = np.array([4,5,6])\n",
    "\n",
    "c = np.stack((a,b), axis=0)\n",
    "d = np.stack((a,b), axis=1)\n",
    "print(\"Vertically Stacked Array:\\n\", c)\n",
    "print(print(\"Horizontally Stacked Array:\\n\", d))\n"
   ]
  }
